{
    "version":2,
    "builds":[
        {
            "src":"Tornament_app/wsgi.py",
            "use":"@vercel/python",
            "config": {"maxLambdaSize" : "15mb" , "runtime" : "python3.11.5" }
        },
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static-build",
            "config" : {
                "distDir":"/TrendNow/static"
            }
        }
    ],
    "routes": [
        {
            "src" : "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Tornament_app/wsgi.py"
        }
    ]
}


{
    "version": 2,
    "builds": [
        {
            "src": "Tornament_app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "/static"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Tornament_app/wsgi.py"
        }
    ]
}
